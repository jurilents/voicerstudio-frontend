{
  "displayName": "voicerstudio",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "start": "BROWSER=none env-cmd -f .env react-scripts start",
    "start:prod": "BROWSER=none env-cmd -f .env.production react-scripts start",
    "start:windows": "set BROWSER=none && env-cmd -f .env react-scripts start",
    "build:dev": "env-cmd -f .env react-scripts build",
    "build:prod": "env-cmd -f .env.production react-scripts build"
  },
  "dependencies": {
    "@ffmpeg/core": "^0.8.5",
    "@ffmpeg/ffmpeg": "^0.9.8",
    "@forlagshuset/simple-fs": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@xzdarcy/react-timeline-editor": "^0.1.9",
    "axios": "^1.4.0",
    "bootstrap": "^5.3.0",
    "core-js": "^3.18.0",
    "date-fns": "^2.30.0",
    "duration-time-conversion": "^1.0.5",
    "env-cmd": "^10.1.0",
    "howler": "^2.2.3",
    "idb": "^7.1.1",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "option-validator": "^2.0.6",
    "react": "^17.0.2",
    "react-bootstrap": "^2.7.4",
    "react-dom": "^17.0.2",
    "react-i18nify": "^4.2.1",
    "react-modal": "^3.16.1",
    "react-redux": "^8.1.0",
    "react-router-dom": "^6.14.0",
    "react-scripts": "^5.0.1",
    "react-textarea-autosize": "^8.3.3",
    "react-toastify": "^9.1.3",
    "react-virtualized": "^9.22.3",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "reselect": "^4.1.8",
    "styled-components": "^5.3.1",
    "uuid": "^9.0.0",
    "wavesurfer.js": "^6.6.4",
    "wfplayer": "^2.1.0"
  },
  "devDependencies": {
    "http-proxy-middleware": "^2.0.6",
    "redux-devtools-extension": "^2.13.9"
  },
  "overrides": {
    "react-error-overlay": "6.0.9"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "react/jsx-uses-react": "off",
      "no-unused-vars": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
